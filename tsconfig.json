{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "vite-plugin-pwa/client",
      "vite-plugin-pwa/info"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}